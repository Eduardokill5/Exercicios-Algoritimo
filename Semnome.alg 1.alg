algoritmo "semnome"
// Função :
// Autor :
// Data : 22/04/2024
// Seção de Declarações 
var
N1, N2, S: Inteiro
inicio
// Seção de Comandos 
    Escreva ("Informe um numero: ")
    Leia(N1)
    Escreva ("Informe outro numero: ")
    Leia (N2)
    S <- N1 + N2
    Escreva (" A soma entre ",N1, " e ", N2, " e igual a ", S)
fimalgoritmo
